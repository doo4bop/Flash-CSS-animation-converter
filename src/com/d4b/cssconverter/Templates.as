package com.d4b.cssconverter{	public class Templates	{				public static var path :String = '';		public static var animation :String = '';		public static var animation_base :String = '';		public static var index :String = '';		public static var image :String = '';				public static function populate ( template:String, data:Object){									for (var key in data){				var reg:RegExp = new RegExp('%%'+key+'%%', 'g');				template = template.replace(reg, data[key] );			}						return template;		}				public function Templates()		{					}	}}