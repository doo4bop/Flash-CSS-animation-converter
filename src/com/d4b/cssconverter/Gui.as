package com.d4b.cssconverter{	import flash.display.MovieClip;	import flash.events.Event;	import fl.controls.CheckBox;	import flash.text.TextField;	import fl.controls.Button;	import fl.controls.TextArea;	import flash.events.MouseEvent;	public class Gui extends MovieClip	{		public static const BATCH_CLICKED:String = 'Gui.click';				public var positionCh	:CheckBox;		public var rotationCh	:CheckBox;		public var scakeCh		:CheckBox;		public var nameField	:TextField;		public var batchBtn		:Button;		public var logTxt		:TextArea;				public function Gui()		{							Config.log = log;			batchBtn.addEventListener(MouseEvent.CLICK, batchClick);		}				public function batchClick (e:MouseEvent){						batchBtn.enabled = false;			Config.animationName = nameField.text;			Config.POSITION_ENABLED = positionCh.selected;			Config.ROTATION_ENABLED = rotationCh.selected;			Config.SCALE_ENABLED = scakeCh.selected;						this.dispatchEvent( new Event ( BATCH_CLICKED) );		}				public function log( msg:String ){			logTxt.text += msg +'\n';		}	}}